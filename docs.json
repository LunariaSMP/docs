{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#9271C5",
    "light": "#9271C5",
    "dark": "#9271C5"
  },
  "favicon": "/PINK_Logo.png",
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://mintlify.com/blog"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Wiki",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      }
    ]
  },
  "logo": "/PINK_Logo.png",
  "navbar": {
    "links": [],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.lunariasmp.de/",
      "youtube": "https://youtube.lunariasmp.de/",
      "website": "https://lunariasmp.de/"
    }
  },
  "background": {
    "decoration": "gradient"
  },
  "appearance": {
    "default": "dark"
  },
  "thumbnails": {
    "appearance": "dark",
    "background": "/PINK_Logo.png"
  },
  "styling": {
    "codeblocks": "dark",
    "latex": true
  }
}
